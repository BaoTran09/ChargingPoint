@using ChargingPoint.ViewModels;
@model VerifyEmailView;

@{
    ViewData["Title"] = "Email Sent";
    Layout = "~/Views/Shared/_AccountLayout.cshtml";
}

<div class="Account-contrainer">
    <div class="Account-box">
        <h2 class="text-center mb-4"> Email Send </h2>
        <p class="text-cent">A verification email has been sent to your email address. Please check your inbox and follow the message </p>
        <div class="text-center mt-2">
            <a asp-controller="Account" asp-action="Login" class="text-deccoration-none mt-2">Back To Login</a>

        </div> 
    </div>

</div>
@section Scripts {

    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial.cshtml");
    }

}
